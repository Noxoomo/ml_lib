{
  "model": {
    "model_arch": "em_model",
    "e_model": {
      "model_arch": "conv_model",
      "device": "GPU",
      "conv": {
        "model_arch": "SmallNet",
        "device": "GPU"
      },
      "classifier": {
        "num_classes": 2,
        "main": {
          "model_arch": "MLP",
          "dims": [512, 2],
          "device": "GPU"
        }
      }
    },
    "m_model": {
      "model_arch": "Id",
      "num_classes": 2,
      "device": "GPU"
    }
  },

  "dataset": {
    "name": "cifar-10",
    "base_classes": [0, 1, 2, 8, 9],
    "training_limit": -1,
    "test_limit": -1
  },

  "checkpoint_file": "small_net_checkpoint.pt",
  "batch_size": 128,
  "reports_per_epoch": 10,
  "em_iterations": {
    "global_iters": 3,
    "e_iters": 0,
    "m_iters": 0
  },
  "sgd_step": 0.01,
  "dropout": 0.0,
  "device": "GPU",

  "scheduled_param_modifiers": [
    {
      "field": "sgd_step",
      "type": "double",
      "values": [0.01, 0.001, 0.0001],
      "iters": [0, 1, 2]
    },
    {
      "field": "em_iterations.e_iters",
      "type": "int",
      "values": [1, 1, 1],
      "iters": [0, 1, 2]
    }
  ]
}